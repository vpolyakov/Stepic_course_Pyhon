    "n = int(input())\n",
    "a = []\n",
    "k = 0\n",
    "while len(a) < n :\n",
    "    k += 1\n",
    "    for i in range(0, k) :\n",
    "        if len(a) < n :\n",
    "            a.append(k)\n",
    "        else: break\n",
    "if len(a) > 0 :\n",
    "    for l in a :\n",
    "        print(l, '', end='')"
   ]
